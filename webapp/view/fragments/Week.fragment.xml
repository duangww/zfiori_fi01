<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:card="sap.f.cards"
    xmlns:w="sap.ui.integration.widgets"
    xmlns:chart="sap.suite.ui.microchart"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:layout="sap.ui.layout"
    xmlns:cssgrid="sap.ui.layout.cssgrid"
>

    <cssgrid:CSSGrid
        id="idCSSGrid2"
        class="sapUiResponsiveMediumContainerWidth responsiveGrid"
        gridTemplateColumns="1fr 1fr"
        gridTemplateRows="1fr 1fr"
        gridGap="1rem"
    >
        <f:Card
            id="idCard3"
            class="sapUiTinyMargin"
        >
            <f:header>
                <card:Header
                    id="idCardHeader3"
                    title="周付款额和账面余额"
                />
            </f:header>
            <f:content>
                <viz:VizFrame
                    id="idVizFrame3"
                    uiConfig="{applicationSet:'fiori'}"
                    height="400px"
                    width="100%"
                    vizType='line'
                >
                    <viz:dataset>
                        <viz.data:FlattenedDataset
                            id="idFlattenedDataset3"
                            data="{WeekData>/0/chartWeekData}"
                        >
                            <viz.data:dimensions>
                                <viz.data:DimensionDefinition
                                    id="idDimensionDefinition3"
                                    name="年周"
                                    value="{WeekData>yearWeek}"
                                />
                            </viz.data:dimensions>
                            <viz.data:measures>
                                <viz.data:MeasureDefinition
                                    id="idMeasureDefinition7"
                                    name="周付款额"
                                    value="{WeekData>payment}"
                                />
                                <viz.data:MeasureDefinition
                                    id="idMeasureDefinition8"
                                    name="周账面余额"
                                    value="{WeekData>balance}"
                                />
                            </viz.data:measures>
                        </viz.data:FlattenedDataset>
                    </viz:dataset>

                    <viz:feeds>
                        <viz.feeds:FeedItem
                            id='valueAxisFeed7'
                            uid="valueAxis"
                            type="Measure"
                            values="周付款额"
                        />
                        <viz.feeds:FeedItem
                            id='valueAxisFeed8'
                            uid="valueAxis"
                            type="Measure"
                            values="周账面余额"
                        />
                        <viz.feeds:FeedItem
                            id='categoryAxisFeed3'
                            uid="categoryAxis"
                            type="Dimension"
                            values="年周"
                        />
                    </viz:feeds>
                    <viz:dependents>
                        <viz:Popover id="idPopOver3" />
                    </viz:dependents>
                </viz:VizFrame>
            </f:content>
        </f:Card>

        <f:Card
        id="idCard4"
        class="sapUiTinyMargin"
    >
        <f:header>
            <card:Header
                id="idCardHeader4"
                title="周订单接收数量"
            />
        </f:header>
        <f:content>
            <viz:VizFrame
                id="idVizFrame4"
                uiConfig="{applicationSet:'fiori'}"
                height="400px"
                width="100%"
                vizType='line'
            >
                <viz:dataset>
                    <viz.data:FlattenedDataset
                        id="idFlattenedDataset4"
                        data="{WeekData>/0/chartWeekData}"
                    >
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition
                                id="idDimensionDefinition4"
                                name="周"
                                value="{WeekData>yearWeek}"
                            />
                        </viz.data:dimensions>
                        <viz.data:measures>
                            <viz.data:MeasureDefinition
                                id="idMeasureDefinition9"
                                name="订单接收数量"
                                value="{WeekData>order}"
                            />
                            <viz.data:MeasureDefinition
                                id="idMeasureDefinition10"
                                name="订单接收电芯数量"
                                value="{WeekData>order_ferth}"
                            />
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>

                <viz:feeds>
                    <viz.feeds:FeedItem
                        id='valueAxisFeed9'
                        uid="valueAxis"
                        type="Measure"
                        values="订单接收数量"
                    />
                    <viz.feeds:FeedItem
                        id='valueAxisFeed10'
                        uid="valueAxis"
                        type="Measure"
                        values="订单接收电芯数量"
                    />
                    <viz.feeds:FeedItem
                        id='categoryAxisFeed4'
                        uid="categoryAxis"
                        type="Dimension"
                        values="周"
                    />
                </viz:feeds>
                <viz:dependents>
                    <viz:Popover id="idPopOver4" />
                </viz:dependents>
            </viz:VizFrame>
        </f:content>
    </f:Card>
    </cssgrid:CSSGrid>
</core:FragmentDefinition> 